<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.or g/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.sysettings.mapper.DictChannelTypeMapper">
    <resultMap id="BaseResultMap" type="org.springblade.dictionary.DictChannelType">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="text" jdbcType="VARCHAR" property="text"/>
        <result column="enabled" jdbcType="INTEGER" property="enabled"/>
        <result column="color" jdbcType="VARCHAR" property="color"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, text, enabled,color
    </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from dict_channel_type
        where id = #{id,jdbcType=INTEGER}
    </select>
    <select id="selectAll" resultType="org.springblade.dictionary.DictChannelType">
        select * from dict_channel_type
    </select>

    <update id="updateByIds" parameterType="org.springblade.dictionary.DictChannelType">
        update dict_channel_type set enabled = #{enabled,jdbcType=INTEGER}
        where 1=1 and id = #{id,jdbcType=INTEGER}
    </update>
    <insert id="add" parameterType="org.springblade.dictionary.DictChannelType">
        insert into dict_channel_type (id, text, enabled,color)
        values (#{id,jdbcType=INTEGER}, #{text,jdbcType=VARCHAR}, #{enabled,jdbcType=INTEGER},#{color,jdbcType=VARCHAR})
    </insert>

</mapper>
